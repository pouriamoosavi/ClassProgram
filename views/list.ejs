<table class="table table-striped">
  <thead>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Status</th>
    <th>Present</th>
  </tr>
  </thead>
  <tbody>
  <% students.forEach(function (student) {  %>
  <tr>
    <td><%=student.firstName%></td>
    <td><%=student.lastName%></td>
    <td>
      <%if(student.present){%>
        Present
      <% }else{%>
        Absent
      <%};%>
    </td>
    <td>
      <a href="/students/present/<%=student.id%>" class="btn btn-success">Present</a>
      <a href="/students/absent/<%=student.id%>" class="btn btn-success">Absent</a>
    </td>
  </tr>
  <%});%>
  </tbody>
</table>
